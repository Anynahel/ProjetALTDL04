{% extends 'base.html.twig' %}

{% block body %}
    <h1>Productorder</h1>

    <table>
        <tbody>
            <tr>
                <th>Date</th>
                <td>{{ productorder.date }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ productorder.price }}</td>
            </tr>
            <tr>
                <th>Idorder</th>
                <td>{{ productorder.idOrder }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('productorder_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('productorder_edit', { 'idOrder': productorder.idOrder }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
